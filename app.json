{
  "expo": {
    "name": "PCDC Teen Club Connect",
    "slug": "pcdc-teen-club-connect",
    "version": "0.1.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0F17"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "appleMobileWebApp": {
        "title": "YouthConnect",
        "capable": true,
        "statusBarStyle": "black-translucent"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B0F17"
      },
      "package": "com.yzhol.pcdcteenclubconnect"
    },
    "web": {
      "bundler": "webpack",
      "output": "single",
      "favicon": "./public/icons/icon-192.png",
      "name": "PCDC Teen Club Connect",
      "shortName": "YouthConnect",
      "lang": "en",
      "themeColor": "#0b0b0b",
      "backgroundColor": "#0b0b0b",
      "display": "standalone",
      "scope": "/",
      "startUrl": "/?source=pwa",
      "orientation": "portrait",
      "serviceWorker": "./public/sw.js",
      "icons": [
        {
          "src": "/icons/icon-192.png",
          "sizes": "192x192",
          "type": "image/png"
        },
        {
          "src": "/icons/icon-512.png",
          "sizes": "512x512",
          "type": "image/png"
        },
        {
          "src": "/icons/icon-512-maskable.png",
          "sizes": "512x512",
          "type": "image/png",
          "purpose": "maskable"
        }
      ],
      "meta": {
        "description": "The official mobile app for the PCDC Teen Club.",
        "theme_color": "#0b0b0b",
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "black-translucent",
        "apple-mobile-web-app-title": "YouthConnect",
        "apple-touch-icon": "/icons/icon-192.png"
      }
    },
    "extra": {
      "SUPABASE_URL": "https://uuqejogbgddmcnirrqkn.supabase.co",
